{
  "name": "vertex-surface",
  "author": "Oleksandr Pastukhov",
  "license": "MIT",
  "version": "1.2.1",
  "repository": "https://github.com/flashua/3D-Surface.git",
  "description": "3D Surface",
  "main": "src/index.js",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "three": "^0.92.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "react-scripts": "1.1.4",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prettier": "prettier --write \"./**/*.js\""
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "semi": false
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "indent": [
        1,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "semi": [
        2,
        "never"
      ]
    }
  }
}
