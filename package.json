{
  "name": "reactor",
  "version": "0.0.1",
  "description": "A React/Flux app generate by RF, powered with Babel",
  "main": "build/bundle.js",
  "dependencies": {
    "flux": "^2.1.1",
    "lodash": "^3.10.1",
    "material-ui": "^0.14.3",
    "object-assign": "^4.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-tap-event-plugin": "^0.2.1",
    "three": "^0.73.0",
    "three-orbit-controls": "^72.0.0"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.8.2",
    "jscs": "^2.5.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "react-tools": "^0.13.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js --colors --content-base ./build",
    "start": "webpack-dev-server --config webpack.dev.config.js --colors --content-base ./build",
    "test": "jest"
  },
  "author": "",
  "license": "",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
